import java.util.Scanner;
public class Main{
    public static Scanner sc = new Scanner(System.in);
    public static String choice;
    public static int product;
    public static int sum = 0;
    public static int sum2 = 0;
    public static int sum3 = 0;
    public static boolean valid = false;
    public static int ctr = 0;
	public static void main(String[] args) {
		Ask_User();
	}
	public static void Ask_User(){
	    System.out.print("Enter a your pin: ");
	    choice = sc.nextLine();
	    char c[] = choice.toCharArray();
	    
	    for(int i = c.length - 2;i >= 0;i=-2){
	        product = c[i] * 2;
	        sum = sum + product;
	    }
	    
	    for(int j = c.length - 1;j >= 0 ; j=-2){
	        sum2 = sum2 + c[j];
	    }
	    
	    sum3 = sum + sum2;
	    
	    if(sum3 %10 == 0){
	        valid = true;
	        if(valid == true){
	            for(int x = 0;x < c.length;x++){
	                ctr++;
	            }
	            
	            if(ctr == 13 && c[0] == 4){
	                System.out.println("VISA");
	            } else if(ctr == 15 && (c[0] == 3 && (c[1] == 3 || c[1] == 7))){
	                System.out.println("AMEX");
	            } else if((ctr == 13 || ctr == 16) && (c[0] == 5 && (c[1] == 1 || c[1] == 2 || c[1] == 3 || c[1] == 4 || c[1] == 5))){
	                System.out.println("MASTERCARD");
	            } else{
	                System.out.println("Invalid Try again");
	            }
	            
	        }
	    }else{
	                System.out.println("Invalid Try again");
	    } 
	    
	}
}
