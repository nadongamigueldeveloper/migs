package reverse;

public class reverse {

	public static void main(String[] args) {
		
		         int [][]arr = {{5,6,3,2}, {7,4,9,8}};

		         for(int i = 0; i < arr.length;i++){
		             for(int j = arr[i].length - 1; j >= 0 ; j--) {
		            	 System.out.print(arr[i][j] + " ");
		             }
		             System.out.println();
		         }
		        
   }    
		     
	

}
