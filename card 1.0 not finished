package card;
import java.util.Scanner;
public class card {
	public static String choice;
	public static int finalsum = 0;
	public static int sum2 = 0;
	public static char ascii = 48;
	public static int sum = 0;
	public static Scanner sc = new Scanner(System.in);

	public static void main(String[] args) {
		CheckValid();

	}
	public static void CheckValid() {
	
			System.out.print("Enter your pin: ");
			choice = sc.nextLine();
			 char c[] = choice.toCharArray();
		
		
		for(int i = c.length - 2;i >= 1;i-=2) {
		 int   convert = c[i] - 48;
		 int   prod = (convert * 2);
		  if(prod > 9) {
		 	prod = (prod % 10) + (prod / 10);
		  }
		   sum = sum + prod;
		}
		
		for(int j = c.length - 1 ;j >= 1;j-=2){
		   int  convert2 = c[j] - 48;
		     sum2 = sum2 + convert2;
		}
		
		finalsum = sum + sum2;
		
		if(finalsum % 10 == 0){
		   if((c.length  == 13 || c.length  == 16) && c[0] == '4') {
					
		   	System.out.println("VISACARD");
			
	       }
		   if(c.length  == 15 &&  (c[0] == '3' && (c[1] == '8' || c[1] == '7')) ) {
				
			System.out.println("AMEX");
		 
	       }
		   if(c.length == 16 && (c[0] == '5' && (c[1] == '1' || c[1] == '2' || c[1] == '3' || c[1] == '4' || c[1] == '5'))) {
				
			System.out.println("MASTERCARD");
           }
			
		}else{
		    System.out.print("Invalid Try again");
		}

	
	   
		
		
	}
}
